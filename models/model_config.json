{
  "architecture": {
    "backbone": "EfficientNetB0",
    "input_shape": [
      224,
      224,
      3
    ],
    "num_classes": 20,
    "classifier_layers": [
      512,
      256
    ],
    "dropout_rates": [
      0.5,
      0.3
    ],
    "output_activation": "sigmoid"
  },
  "training": {
    "loss": "binary_crossentropy",
    "optimizer": "Adam",
    "learning_rate": 0.0005,
    "batch_size": 32,
    "epochs": 50
  },
  "metrics": [
    "accuracy",
    "precision",
    "recall",
    "auc",
    "hamming_loss",
    "f1_micro",
    "f1_macro"
  ],
  "classes": [
    "aeroplane",
    "bicycle",
    "bird",
    "boat",
    "bottle",
    "bus",
    "car",
    "cat",
    "chair",
    "cow",
    "diningtable",
    "dog",
    "horse",
    "motorbike",
    "person",
    "pottedplant",
    "sheep",
    "sofa",
    "train",
    "tvmonitor"
  ]
}